<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= link_to "add", add_user_team_path(team: @team, :user_id => user.id, team_id: @team.id), :method => :put %></td>
      </tr>
    <% end %>
  </tbody>
</table>
